<%= will_paginate @collection, renderer: BootstrapPagination::Rails %>
<table class='table table-striped table-condensed'>
  <thead>
    <tr>
      <th>Barcode</th>
      <th>Item Description</th>
      <th>Item Price</th>
    </tr>
  </thead>

  <tbody>
    <% @barcodes.each do |barcode| %>
      <tr>
        <td><%= barcode.id %></td>
        <td><%= barcode.itemfile.Item_Description_1 %></td>
        <td><%= barcode.itemfile.Base_Unit_Retail %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<hr/>

<%= react_component('Upload', tokenName: request_forgery_protection_token, token: form_authenticity_token)%>
